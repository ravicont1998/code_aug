    <script src="Scripts/actions.py" />
  </scripts>
  <fonts>
  </fonts>
  <proxygen definitionsrc="Proxy/proxydef.xml">
  </proxygen>
  <globalvariables>
    <globalvariable name="gameEnded" value="False" />
