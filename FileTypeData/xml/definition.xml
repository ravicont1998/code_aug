    <group name="Magic Pile" visibility="none" ordered="False" width="63" height="88" icon="Groups/Magic-Pile.png" collapsed="False">
    </group>
    <group name="Discard Pile" visibility="all" ordered="False" width="63" height="88" icon="Groups/Discard-Pile.png" collapsed="False">
    </group>
  </player>
  <shared>
    <group name="Exile" visibility="all" ordered="False" width="63" height="88" icon="Groups/Exile.png" collapsed="False" />
  </shared>
  <deck>
    <section name="Hand" group="Hand" />
