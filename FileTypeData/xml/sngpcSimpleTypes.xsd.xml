<schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:sngpc="urn:sngpc-schema"
    targetNamespace="urn:sngpc-schema" elementFormDefault="qualified">
  <!-- Schema com os tipos simples importados do TISS -->
  <simpleType name="st_classeTerapeutica">
    <restriction base="string">
      <!--Antimicrobiano-->
      <enumeration value="1" />
      <!--Sujeito a controle Especial-->
      <enumeration value="2" />
    </restriction>
  </simpleType>
  <simpleType name="st_data">
    <restriction base="date"></restriction>
  </simpleType>
  <simpleType name="st_CPF">
    <restriction base="string">
      <!-- O tamanho M?nimo tamb?m deve ser 11 -->
      <!-- <minLength value="0" />  -->
      <minLength value="11" />
      <maxLength value="11" />
      <whiteSpace value="collapse" />
      <pattern value="\d{11}"></pattern>
    </restriction>
  </simpleType>
  <simpleType name="st_CNPJ">
    <restriction base="string">
      <!-- O Tamanho m?nimo deve tamb?m ser 14 -->
      <minLength value="14" />
      <maxLength value="14" />
      <pattern value="\d{14}" />
    </restriction>
  </simpleType>
  <simpleType name="st_RazaoSocial">
    <restriction base="string">
      <maxLength value="100" />
    </restriction>
  </simpleType>
  <simpleType name="st_AES">
    <restriction base="string">
      <minLength value="3" />
      <maxLength value="3" />
      <pattern value="[0-9]{3}" />
    </restriction>
  </simpleType> 
  <simpleType name="st_simNao">
    <restriction base="string">
      <!--  Sim -->
      <enumeration value="S" />
      <!--  Nao -->
      <enumeration value="N" />
    </restriction>
  </simpleType>
  <simpleType name="st_SimNaoNull">
    <restriction base="string">
      <!--  Sim -->
      <enumeration value="S" />
      <!--  Nao -->
      <enumeration value="N" />
      <!-- Vazio -->
      <enumeration value="" />
    </restriction>
  </simpleType>
  <!-- Schema com os tipos simples de dados re-utilizados na constru??o dos tipos complexos -->
  <simpleType name="st_NumeroNotaFiscal">
    <restriction base="integer">
      <totalDigits value="9" />
    </restriction>
  </simpleType>
  <simpleType name="st_QuantidadeMedicamento">
    <restriction base="integer">
      <totalDigits value="6"/>
      <minExclusive value="0"/>
    </restriction>
  </simpleType>
  <simpleType name="st_QuantidadeGramasMedicamento">
    <restriction base="decimal">
      <totalDigits value="5" />
      <fractionDigits value="2" />
      <maxExclusive value="1000" />
      <minExclusive value="0"/>
    </restriction>
  </simpleType>
  <simpleType name="st_Notificacao">
    <restriction base="string">
      <maxLength value="10" />
    </restriction>
  </simpleType>
  <simpleType name="st_RegistroMS">
    <restriction base="string">
      <minLength value="13" />
      <maxLength value="14" />
      <pattern value="[1]{1}[0-9]{12,13}" />
    </restriction>
  </simpleType>
  <simpleType name="st_CodigoDCB">
    <restriction base="string">
      <length value="5"></length>
      <pattern value="\d{5}"></pattern>
    </restriction>
  </simpleType>
  <simpleType name="st_Nome">
    <restriction base="string">
      <maxLength value="100" />
    </restriction>
  </simpleType>
  <simpleType name="st_NumeroDocumento">
    <restriction base="string">
      <maxLength value="30" />
    </restriction>
  </simpleType>
  <simpleType name="st_Lote">
    <restriction base="string">
      <maxLength value="20" />
    </restriction>
  </simpleType>
  <simpleType name="st_QuantidadeInsumo">
    <restriction base="decimal">
      <totalDigits value="12" />
      <fractionDigits value="6" />
      <maxExclusive value="1000000" />
      <minExclusive value="0"/>
    </restriction>
  </simpleType>
  <simpleType name="st_QuantidadeDeInsumoPorUnidadeFarmacotecnica">
    <restriction base="decimal">
      <totalDigits value="15" />
      <fractionDigits value="6" />
      <maxExclusive value="1000000000" />
      <minExclusive value="0"/>
    </restriction>
  </simpleType>
  <simpleType name="st_QuantidadeDeUnidadesFarmacotecnicas">
    <restriction base="decimal">
      <totalDigits value="10" />
      <fractionDigits value="6" />
      <maxExclusive value="10000" />
      <minExclusive value="0"/>
    </restriction>
  </simpleType>
  <simpleType name="st_TipoReceituario">
    <restriction base="string">
      <!--Receita de Controle Especial em 2 vias (Receita Branca)-->
      <enumeration value="1" />
      <!--Notifica??o de Receita B (Notifica??o Azul)-->
      <enumeration value="2" />
      <!--Notifica??o de Receita Especial (Notifica??o Branca)-->
      <enumeration value="3" />
      <!--Notifica??o de Receita A (Notifica??o Amarela)-->
      <enumeration value="4" />
	  <!--Receita Antimicrobiano em 2 vias-->
	  <enumeration value="5"/> 
    </restriction>
  </simpleType>
  <simpleType name="st_TipoUsoMedicamento">
    <restriction base="string">
      <!--Humano-->
      <enumeration value="1" />
      <!--Veterinario-->
      <enumeration value="2" />
    </restriction>
  </simpleType>
  <simpleType name="st_TipoOperacaoNotaFiscal">
    <restriction base="string">
      <!--Compra-->
      <enumeration value="1" />
      <!--Transfer?ncia-->
      <enumeration value="2" />
      <!--Venda-->
      <enumeration value="3" />
    </restriction>
  </simpleType>
  <simpleType name="st_ConselhoProfissional">
    <restriction base="string">
      <!--- Conselho Regional de Medicina -->
      <enumeration value="CRM" />
      <!-- Conselho Regional de Medicina Veterin?ria -->
      <enumeration value="CRMV" />
      <!-- Conselho Regional de Odontologia -->
      <enumeration value="CRO" />
	  <!--
      Conselho Regional de Enfermagem
	  <enumeration value="COREN"/>
    -->
	  <!-- Conselho Regional de Farm?cia -->   
	  <enumeration value="CRF"/>
	  
	  <!-- Programa Mais Medicos - Registro Minist?rio da Sa?de -->
	  <enumeration value="RMS"/>
    </restriction>
  </simpleType>
  <simpleType name="st_UF">
    <restriction base="string">
      <!--- Acre -->
      <enumeration value="AC" />
      <!-- Alagoas -->
      <enumeration value="AL" />
      <!-- Amazonas -->
      <enumeration value="AM" />
      <!-- Amapa -->
      <enumeration value="AP" />
      <!-- Bahia -->
      <enumeration value="BA" />
      <!-- Ceara -->
      <enumeration value="CE" />
      <!-- Distrito Federal -->
      <enumeration value="DF" />
      <!-- Espirito Santo -->
      <enumeration value="ES" />
      <!-- Goias -->
      <enumeration value="GO" />
      <!-- Maranhao -->
      <enumeration value="MA" />
      <!-- Minas Gerais -->
      <enumeration value="MG" />
      <!-- Mato Grosso do Sul -->
      <enumeration value="MS" />
      <!-- Mato Grosso -->
      <enumeration value="MT" />
      <!-- Para -->
      <enumeration value="PA" />
      <!-- Paraiba -->
      <enumeration value="PB" />
      <!-- Pernambuco -->
      <enumeration value="PE" />
      <!-- Piaui -->
      <enumeration value="PI" />
      <!-- Parana -->
      <enumeration value="PR" />
      <!-- Rio de Janeiro -->
      <enumeration value="RJ" />
      <!-- Rio Grande do Norte -->
      <enumeration value="RN" />
      <!-- Rondonia -->
      <enumeration value="RO" />
      <!-- Roraima -->
      <enumeration value="RR" />
      <!-- Rio Grande do Sul -->
      <enumeration value="RS" />
      <!-- Santa Catarina -->
      <enumeration value="SC" />
