  <simpleType name="st_DescricaoValidacao">
    <restriction base="string">
      <length value="200" />
    </restriction>
  </simpleType>
  <simpleType name="st_Idade">
    <restriction base="integer">
      <minInclusive value="0"/>
      <maxInclusive value="999"/>      
    </restriction>    
  </simpleType>

  <simpleType name="st_UnidadeIdade">
    <restriction base="int">
      <!-- Anos -->
      <enumeration value="1" />
      <!-- Meses -->
      <enumeration value="2" />
    </restriction>
  </simpleType>

  <simpleType name="st_Sexo">
    <restriction base="int">
      <!-- Masculino -->
      <enumeration value="1" />
      <!-- Feminino -->
      <enumeration value="2" />     
    </restriction>
  </simpleType>

  <!--Tipo e tamanho definidos em conformidade com a 
      Tabela CID10 do Schema de Banco de Dados Coorporativo-->
  <simpleType name="st_Cid">
    <restriction base="string">
       <minLength value="0" />
      <maxLength value="4" />
    </restriction>
  </simpleType>


  <simpleType name="st_UnidadeMedidaMedicamento">
    <restriction base="string">
