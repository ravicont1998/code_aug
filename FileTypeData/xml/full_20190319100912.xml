<assembly>
  <id>full</id>
  <formats>
    <format>tar.gz</format>
    <format>tar.bz2</format>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <moduleSets>
    <moduleSet>
      <includes>
        <include>net.kodeninja:jem-common</include>
        <include>net.kodeninja:jem-server</include>
      </includes>
      <binaries>
        <outputDirectory></outputDirectory>
        <unpack>false</unpack>
      </binaries>
    </moduleSet>
    <moduleSet>
      <excludes>
        <exclude>net.kodeninja:jem-common</exclude>
        <exclude>net.kodeninja:jem-server</exclude>
      </excludes>
      <binaries>
        <outputDirectory>plugins</outputDirectory>
        <unpack>false</unpack>
      </binaries>
    </moduleSet>
  </moduleSets>
</assembly>